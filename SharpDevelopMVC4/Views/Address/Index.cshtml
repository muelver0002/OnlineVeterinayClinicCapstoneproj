@model List<SharpDevelopMVC4.Models.Address>
@{
	ViewBag.Title = "Index";
	Layout = "~/Views/Shared/_Layout.cshtml";
}
 <head>
<style>
table {
  font-family: arial, sans-serif;
  border-collapse: collapse;
  width: 100%;
  
}

td, th {
  border: 1px solid #dddddd;
  text-align: left;
  padding: 8px;
}


</style>
</head>
    <div class="table-responsive">
 <h1 style="margin-bottom: 40px;margin-top: 40px;    margin-bottom:  40px;
   text-transform: uppercase;
   font-weight: bold;
   letter-spacing: 1px;">Location Registered List</h1>

<table class="table table-striped table-dark table-hover">
        <thead>
            <tr>
               
                <th style="font-size: 30px;">Barangay</th>
              
            </tr>
        </thead>
        <tbody>
			@foreach(var barangay in Model)
			{
	            <tr>
	                   
	                <td>@barangay.Addname</td>
               
	            </tr>
			}
        </tbody>
    </table>