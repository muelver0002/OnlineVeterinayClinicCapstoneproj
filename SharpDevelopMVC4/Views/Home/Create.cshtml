@{
	ViewBag.Title = "Create";
	Layout = "~/Views/Shared/_Layout.cshtml";
}

 <head>
        <title>Bootstrap Datepicker Disable Past Dates Example - itsolutionstuff.com/</title>
        <link rel="stylesheet" type="text/css" href="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/4.3.1/css/bootstrap.css">
        <link rel="stylesheet" type="text/css" href="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-datepicker/1.9.0/css/bootstrap-datepicker.css">
        <script src = "https://ajax.googleapis.com/ajax/libs/jquery/2.1.3/jquery.min.js"></script>
        <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-datepicker/1.9.0/js/bootstrap-datepicker.js"></script>
        <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js"></script>
    </head>
 @if (Request.IsAuthenticated)
     {
<h2>Create</h2>

<div class="row">
    <div class="col-md-4">
        <section>
        
            <form action="/pet/create" method="post">
            	 
            	 <div class="form-group">
                    <label>Name<label style="color:red">*</label>
                    <input class="form-control" type="text" id="PetName" name="PetName" required />
                </div>  
               
              
                
                 <div class="form-group">
                    <label>Breed<label style="color:red">*</label>
                    <input class="form-control" type="text" id="Breed" name="Breed"  required/>
                   
                </div>

                <div class="form-group">
                    <label>Type of pet<label style="color:red">*</label>
                    <input class="form-control" type="text" id="Type" name="Type" required/>
                </div>
                
                <div class="form-group">
                    <label>Color<label style="color:red">*</label>
                    <input class="form-control" type="text" id="Color" name="Color" required/>
                </div>
                
                 <div class="form-group">
                     <label>Blood Type<label style="color:red">*</label>
                    <input class="form-control" type="text" id="Bloodtype" name="Bloodtype" required/>
                </div>
                
                 <div class="form-group">
   
				   <label>Gender<label style="color:red">*</label>
				   <select name="Gender" class="form-control" required>
				   <option>Gender</option>
				  
				    <option value="Male">Male</option>
				    <option value="Female">Female</option>
				      
				   </select>
				   
				 </div>
                
                 <div class="form-group">
                    <label>Birth Date<label style="color:red">*</label>
                    <input type="text" class="form-control datepicker" type="Date" id="Bdate" name="Bdate" required/>
                </div>
              
             
                <div class="form-group">
                  
                    <button type="submit" class="btn btn-primary" onclick="trytoredirect()">Create </button>
                     <a href="/Home/index" class="btn btn-secondary ">Cancel</a>
                     <p style="color:red">@ViewBag.message</p>
                </div>
                <div class="form-group">
                </div>				               
            </form>
            
        </section>
    </div>


</div>

<script type="text/javascript">
   
    $('.datepicker').datepicker({ 
        endDate: new Date()
    });
  
</script>





<script>
function trytoredirect(){
$('.divLoading').show();
  setTimeout(redirect,3000); 
// show loading in 3 secound before redirect this is example
  }
</script>
<script>
function redirect() {
    location.href = "";
}
</script>
<script src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.1/jquery.min.js"></script>

<!-- loading from fancy box -->
<style>.Panel-Loading-BG{height:100%;width:100%;position:fixed;left:0;top:0;z-index:8010;background:url('data:image/png;base64,')}#Panel-Loading{background-image:url('data:image/png;base64,');position:fixed;top:50%;left:50%;margin-top:-22px;margin-left:-22px;background-position:0 -108px;opacity:0.8;cursor:pointer;z-index:8060}#Panel-Loading div{width:44px;height:44px;background:url('data:image/gif;base64,') center center no-repeat}</style><div class="divLoading" style="display:none"><div class="Panel-Loading-BG"></div><div id="Panel-Loading"><div></div></div></div>



}