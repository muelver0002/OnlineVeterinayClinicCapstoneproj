@model List<SharpDevelopMVC4.Models.Receptionist>
@{
	ViewBag.Title = "Index";
	Layout = "~/Views/Shared/_Layout.cshtml";
}
 @if (Request.IsAuthenticated)
     {
<head>
<style>
table {
  font-family: arial, sans-serif;
  border-collapse: collapse;
  width: 100%;
  
}

td, th {
  border: 1px solid #dddddd;
  text-align: left;
  padding: 8px;
}


</style>
</head>
<div class="table-responsive">
 <h1 style="margin-bottom: 40px;margin-top: 40px;    margin-bottom:  40px;
   text-transform: uppercase;
   font-weight: bold;
   letter-spacing: 1px;">Registered Receptionist Lists</h1>
   
 <table class="table table-striped table-dark table-hover">
        <thead>
            <tr>
                
                <th>FullName</th>
               
                 
                <th>UserNme</th>
                <th>Password</th>
                <th>Role</th>
               <!-- <th>Action</th>-->
                 
                 
            </tr>
        </thead>
        <tbody>
			@foreach(var product in Model)
			{
	            <tr>
	                
	                <td>@product.Fullname</td>
	               
	                <td>@product.Username</td>
	                <td>@product.Password</td>
	                <!--<td><a href="/Receptionist/Delete/@product.Id" class="btn btn-danger btn-sm" onclick="return confirm('Are you sure?')"><i class="fa fa-trash"></i></a></td>-->
	                  <td>@product.Role</td>
	                
	                 
	                 
	                 
	            </tr>
			}
        </tbody>
    </table>
    </div>
    }